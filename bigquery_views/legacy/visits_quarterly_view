select
  s.name AS site_name,
  CONCAT(c.group, " ", c.type) AS category_name,
  v.quarter_start AS quarter_start,
  v.count AS count
from `mnhs-dw-prod`.dw.quarterly_visits as v
join `mnhs-dw-prod`.dw.sites as s on v.site_uuid = s.uuid
join `mnhs-dw-prod`.dw.visit_categories as c on v.category_uuid = c.uuid
ORDER BY site_name, category_name, quarter_start